{
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [">0.25%", "not ie 11", "not op_mini all"],
          "uglify": true
        },
        "useBuiltIns": true
      }
    ],
    "react"
  ],

  "plugins": [
    "babel-plugin-styled-components",
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    ["transform-class-properties", { "spec": true }]
  ]
}
